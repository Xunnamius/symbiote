{
  "name": "@-xun/scripts",
  "version": "1.13.0",
  "description": "The scripts powering Xunnamius's NPM-based projects",
  "keywords": [
    "Xunnamius"
  ],
  "homepage": "https://github.com/Xunnamius/xscripts",
  "repository": {
    "type": "git",
    "url": "https://github.com/Xunnamius/xscripts.git"
  },
  "license": "MIT",
  "author": "Xunnamius",
  "sideEffects": false,
  "type": "commonjs",
  "exports": {
    ".": {
      "types": "./dist/src/cli.d.ts",
      "default": "./dist/src/cli.js"
    },
    "./commands/*": {
      "types": "./dist/src/commands/*",
      "default": "./dist/src/commands/*"
    },
    "./configure": {
      "types": "./dist/src/configure.d.ts",
      "default": "./dist/src/configure.js"
    },
    "./package": "./package.json",
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "index": [
        "dist/src/cli.d.ts"
      ],
      "commands/*": [
        "dist/src/commands/*"
      ],
      "configure": [
        "dist/src/configure.d.ts"
      ],
      "package": [
        "package.json"
      ]
    }
  },
  "bin": {
    "xscripts": "./dist/src/cli.js"
  },
  "files": [
    "/dist",
    "/LICENSE",
    "/package.json",
    "/README.md"
  ],
  "scripts": {
    "build": "NODE_NO_WARNINGS=1 ./node_modules/@-xun/scripts/dist/src/cli.js build",
    "build:changelog": "NODE_NO_WARNINGS=1 ./node_modules/@-xun/scripts/dist/src/cli.js build changelog",
    "build:dist": "NODE_NO_WARNINGS=1 ./node_modules/@-xun/scripts/dist/src/cli.js build distributables",
    "build:docs": "NODE_NO_WARNINGS=1 ./node_modules/@-xun/scripts/dist/src/cli.js build docs",
    "build:externals": "NODE_NO_WARNINGS=1 ./node_modules/@-xun/scripts/dist/src/cli.js build externals",
    "clean": "NODE_NO_WARNINGS=1 ./node_modules/@-xun/scripts/dist/src/cli.js clean",
    "format": "NODE_NO_WARNINGS=1 ./node_modules/@-xun/scripts/dist/src/cli.js format --hush",
    "lint": "stdbuf -i0 -o0 -e0 tsc --project tsconfig.lint.json; X=$?; stdbuf -i0 -o0 -e0 eslint --parser-options=project:tsconfig.lint.json --no-error-on-unmatched-pattern packages src; Y=$?; MD_FILES=$(node -e 'console.log(require(`glob-gitignore`).sync(`**/*.md`, { ignore: require(`fs`).readFileSync(`.prettierignore`, `utf8`).split(`\n`).filter(Boolean), dot: true }).join(`\n`))') && echo $MD_FILES | NODE_ENV=lint xargs remark --quiet --frail --no-stdout; Z=$?; [ $X -eq 0 ] && [ $Y -eq 0 ] && [ $Z -eq 0 ]",
    "lint:all": "stdbuf -i0 -o0 -e0 tsc --project tsconfig.eslint.json; X=$?; stdbuf -i0 -o0 -e0 eslint --parser-options=project:tsconfig.eslint.json .; Y=$?; MD_FILES=$(node -e 'console.log(require(`glob-gitignore`).sync(`**/*.md`, { ignore: require(`fs`).readFileSync(`.prettierignore`, `utf8`).split(`\n`).filter(Boolean), dot: true }).join(`\n`))') && echo $MD_FILES | NODE_ENV=lint xargs remark --quiet --frail --no-stdout; Z=$?; [ $X -eq 0 ] && [ $Y -eq 0 ] && [ $Z -eq 0 ]",
    "list-tasks": "NODE_NO_WARNINGS=1 ./node_modules/@-xun/scripts/dist/src/cli.js list-tasks",
    "prepare": "NODE_NO_WARNINGS=1 ./node_modules/@-xun/scripts/dist/src/cli.js prepare",
    "start": "NODE_NO_WARNINGS=1 ./node_modules/@-xun/scripts/dist/src/cli.js start --",
    "test": "npm run test:unit --",
    "test:all": "attw --pack . && NODE_ENV=test DEBUG_COLORS=false jest --coverage",
    "test:e2e": "NODE_ENV=test DEBUG_COLORS=false jest 'e2e.*?\\.test\\.tsx?'",
    "test:integration": "NODE_ENV=test DEBUG_COLORS=false jest '/integration.*?\\.test\\.tsx?'",
    "test:integration:client": "NODE_ENV=test DEBUG_COLORS=false jest '/integration-client.*?\\.test\\.tsx?'",
    "test:integration:compile": "NODE_ENV=test DEBUG_COLORS=false jest '/integration-compile.*?\\.test\\.tsx?'",
    "test:integration:externals": "NODE_ENV=test DEBUG_COLORS=false jest '/integration-external.*?\\.test\\.tsx?'",
    "test:integration:node": "NODE_ENV=test DEBUG_COLORS=false jest '/integration-node.*?\\.test\\.tsx?'",
    "test:repeat:all": "npm run __test:repeat:all --silent",
    "test:repeat:unit": "npm run __test:repeat:unit --silent",
    "test:unit": "NODE_ENV=test DEBUG_COLORS=false jest '/unit-.*\\.test\\.tsx?'"
  },
  "dependencies": {
    "@arethetypeswrong/cli": "^0.16.0",
    "@babel/cli": "^7.24.7",
    "@babel/core": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@black-flag/core": "^1.2.7",
    "@listr2/manager": "^2.0.10",
    "@projector-js/core": "^1.3.1",
    "@types/debug": "^4.1.12",
    "all-contributors-cli": "^6.26.1",
    "askpassword": "^2.0.2",
    "babel-plugin-list-imports": "^1.0.1",
    "conventional-changelog-cli": "https://xunn.at/conventional-changelog-cli@master",
    "core-js": "3.37.1",
    "debug": "^4.3.5",
    "doctoc": "^2.2.1",
    "eslint": "^8.57.0",
    "execa": "^5.1.1",
    "find-up": "^7.0.0",
    "glob": "^10.4.2",
    "glob-gitignore": "^1.0.14",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lodash.clone": "^4.5.0",
    "lodash.clonedeepwith": "^4.5.0",
    "lodash.isequal": "^4.5.0",
    "prettier": "^3.3.2",
    "remark-cli": "^12.0.1",
    "remark-gfm": "^4.0.0",
    "remark-lint-no-undefined-references": "^5.0.0",
    "rimraf": "^5.0.7",
    "semantic-release": "https://xunn.at/semantic-release-atam@22.0.7",
    "sort-package-json": "^2.10.0",
    "supports-color": "^8.1.1",
    "tsconfig-replace-paths": "^0.0.14",
    "type-fest": "^4.21.0",
    "typedoc": "^0.26.3",
    "typescript": "^5.5.3",
    "unique-filename": "^3.0.0"
  },
  "devDependencies": {
    "@-xun/scripts": "^1.13.0",
    "@babel/eslint-parser": "^7.24.7",
    "@babel/plugin-proposal-export-default-from": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/jest": "^29.5.12",
    "@types/lodash.clone": "^4.5.9",
    "@types/lodash.clonedeepwith": "^4.5.9",
    "@types/lodash.isequal": "^4.5.8",
    "@types/lodash.merge": "^4.6.9",
    "@types/node": "^20.14.9",
    "@types/spellchecker": "^3.5.2",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "@xunnamius/conventional-changelog-projector": "^1.2.1",
    "@xunnamius/jest-types": "^1.1.3",
    "babel-jest": "^29.7.0",
    "babel-plugin-explicit-exports-references": "^1.0.2",
    "babel-plugin-module-resolver": "^5.0.2",
    "babel-plugin-transform-rewrite-imports": "^1.2.0",
    "confusing-browser-globals": "^1.0.11",
    "dotenv": "^16.4.5",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-babel-module": "^5.3.2",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^28.6.0",
    "eslint-plugin-module-resolver": "^1.5.0",
    "eslint-plugin-unicorn": "^54.0.0",
    "jest-circus": "^29.7.0",
    "jest-extended": "^4.0.2",
    "lint-staged": "^15.2.7",
    "lodash.merge": "^4.6.2",
    "merge-descriptors": "^2.0.0",
    "remark-capitalize-headings": "^2.0.1",
    "remark-frontmatter": "^5.0.0",
    "remark-ignore": "^2.0.0",
    "remark-lint": "^10.0.0",
    "remark-lint-definition-case": "^4.0.0",
    "remark-lint-fenced-code-flag": "^4.0.0",
    "remark-lint-fenced-code-flag-case": "^2.0.0",
    "remark-lint-file-extension": "^3.0.0",
    "remark-lint-final-newline": "^3.0.0",
    "remark-lint-first-heading-level": "^4.0.0",
    "remark-lint-hard-break-spaces": "^4.0.0",
    "remark-lint-heading-increment": "^4.0.0",
    "remark-lint-heading-whitespace": "^1.0.0",
    "remark-lint-heading-word-length": "^2.0.0",
    "remark-lint-list-item-style": "^2.0.0",
    "remark-lint-no-auto-link-without-protocol": "^3.1.2",
    "remark-lint-no-blockquote-without-marker": "^6.0.0",
    "remark-lint-no-duplicate-defined-urls": "^3.0.0",
    "remark-lint-no-duplicate-definitions": "^4.0.0",
    "remark-lint-no-duplicate-headings-in-section": "^4.0.0",
    "remark-lint-no-empty-sections": "^4.0.0",
    "remark-lint-no-empty-url": "^4.0.0",
    "remark-lint-no-heading-content-indent": "^5.0.0",
    "remark-lint-no-heading-like-paragraph": "^4.0.0",
    "remark-lint-no-heading-punctuation": "^4.0.0",
    "remark-lint-no-inline-padding": "^4.1.2",
    "remark-lint-no-literal-urls": "^4.0.0",
    "remark-lint-no-multiple-toplevel-headings": "^4.0.0",
    "remark-lint-no-reference-like-url": "^4.0.0",
    "remark-lint-no-shell-dollars": "^4.0.0",
    "remark-lint-no-shortcut-reference-image": "^4.0.0",
    "remark-lint-no-shortcut-reference-link": "^4.0.0",
    "remark-lint-no-tabs": "^4.0.0",
    "remark-lint-no-unused-definitions": "^4.0.0",
