{
  "name": "@-xun/scripts",
  "version": "1.20.5",
  "description": "The scripts and assets powering Xunnamius's NPM-based projects",
  "keywords": [
    "Xunnamius"
  ],
  "homepage": "https://github.com/Xunnamius/xscripts",
  "repository": "https://github.com/Xunnamius/xscripts",
  "license": "MIT",
  "author": "Xunnamius",
  "sideEffects": false,
  "type": "commonjs",
  "exports": {
    ".": {
      "types": "./dist/src/cli.d.ts",
      "default": "./dist/src/cli.js"
    },
    "./commands/*": {
      "types": "./dist/src/commands/*.d.ts",
      "default": "./dist/src/commands/*.js"
    },
    "./assets": {
      "types": "./dist/src/assets/index.d.ts",
      "default": "./dist/src/assets/index.js"
    },
    "./assets/config/*": {
      "types": "./dist/src/assets/config/_*.d.ts",
      "default": "./dist/src/assets/config/_*.js"
    },
    "./configure": {
      "types": "./dist/src/configure.d.ts",
      "default": "./dist/src/configure.js"
    },
    "./package": "./package.json",
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "index": [
        "dist/src/cli.d.ts"
      ],
      "commands/*": [
        "dist/src/commands/*.d.ts"
      ],
      "assets": [
        "dist/src/assets/index.d.ts"
      ],
      "assets/config/*": [
        "dist/src/assets/config/_*.d.ts"
      ],
      "configure": [
        "dist/src/configure.d.ts"
      ],
      "package": [
        "package.json"
      ]
    }
  },
  "bin": {
    "xscripts": "./dist/src/cli.js"
  },
  "files": [
    "/dist",
    "/LICENSE",
    "/package.json",
    "/README.md"
  ],
  "scripts": {
    "build": "npm run build:dist --",
    "build:changelog": "NODE_NO_WARNINGS=1 ./node_modules/@-xun/scripts/dist/src/cli.js build changelog",
    "build:dist": "NODE_NO_WARNINGS=1 ./node_modules/@-xun/scripts/dist/src/cli.js build distributables",
    "build:docs": "NODE_NO_WARNINGS=1 ./node_modules/@-xun/scripts/dist/src/cli.js build docs",
    "build:externals": "NODE_NO_WARNINGS=1 ./node_modules/@-xun/scripts/dist/src/cli.js build externals",
    "build:transpiled": "npm run build:transpiled:test",
    "build:transpiled:dev": "NODE_NO_WARNINGS=1 ./node_modules/@-xun/scripts/dist/src/cli.js build distributables --generate-intermediates-for development",
    "build:transpiled:test": "NODE_NO_WARNINGS=1 ./node_modules/@-xun/scripts/dist/src/cli.js build distributables --generate-intermediates-for test",
    "clean": "NODE_NO_WARNINGS=1 ./node_modules/@-xun/scripts/dist/src/cli.js clean",
    "format": "NODE_NO_WARNINGS=1 ./node_modules/@-xun/scripts/dist/src/cli.js format --hush",
    "lint": "npm run lint:source",
    "lint:project": "NODE_NO_WARNINGS=1 ./node_modules/@-xun/scripts/dist/src/cli.js project lint",
    "lint:source": "NODE_NO_WARNINGS=1 ./node_modules/@-xun/scripts/dist/src/cli.js lint",
    "lint:source:all": "NODE_NO_WARNINGS=1 ./node_modules/@-xun/scripts/dist/src/cli.js lint --scope all --run-to-completion",
    "list-tasks": "NODE_NO_WARNINGS=1 ./node_modules/@-xun/scripts/dist/src/cli.js list-tasks",
    "prepare": "NODE_NO_WARNINGS=1 ./node_modules/@-xun/scripts/dist/src/cli.js prepare",
    "release": "NODE_NO_WARNINGS=1 ./node_modules/@-xun/scripts/dist/src/cli.js release",
    "renovate": "NODE_NO_WARNINGS=1 ./node_modules/@-xun/scripts/dist/src/cli.js project renovate",
    "start": "NODE_NO_WARNINGS=1 ./node_modules/@-xun/scripts/dist/src/cli.js start --",
    "stats": "NODE_NO_WARNINGS=1 ./node_modules/@-xun/scripts/dist/src/cli.js project info",
    "test": "npm run test:unit --",
    "test:all": "NODE_NO_WARNINGS=1 ./node_modules/@-xun/scripts/dist/src/cli.js test --coverage",
    "test:all:repeat": "NODE_NO_WARNINGS=1 ./node_modules/@-xun/scripts/dist/src/cli.js test --repeat 99",
    "test:e2e": "NODE_NO_WARNINGS=1 ./node_modules/@-xun/scripts/dist/src/cli.js test --type end-to-end",
    "test:e2e:repeat": "NODE_NO_WARNINGS=1 ./node_modules/@-xun/scripts/dist/src/cli.js test --type end-to-end --repeat 99",
    "test:integration": "NODE_NO_WARNINGS=1 ./node_modules/@-xun/scripts/dist/src/cli.js test --type integration",
    "test:integration:repeat": "NODE_NO_WARNINGS=1 ./node_modules/@-xun/scripts/dist/src/cli.js test --type integration --repeat 99",
    "test:unit": "NODE_NO_WARNINGS=1 ./node_modules/@-xun/scripts/dist/src/cli.js test --type unit",
    "test:unit:repeat": "NODE_NO_WARNINGS=1 ./node_modules/@-xun/scripts/dist/src/cli.js test --type unit --repeat 99"
  },
